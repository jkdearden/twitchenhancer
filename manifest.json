{
    "manifest_version": 2,
    "name": "Twitch player redirector",
    "description": "Redirects video card clicks to chat-less player.",
    "permissions": ["storage"],
    "version": "1.01",
    "content_scripts": [
        {
            "matches": [
                "*://www.twitch.tv/directory/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_start"
        }
    ]
}